            Utility:TweenTransparency(YesIcon, 'img', 1)
            Utility:TweenTransparency(NoButton,"bg",1)
            Utility:TweenTransparency(NoButton,"text",1)
            Utility:TweenTransparency(NoIcon, 'img', 1)
            wait(0.25)
            Template:Destroy()
        end

