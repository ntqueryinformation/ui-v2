    }

    table.insert(CFAHub, title)

    function CFAHub:SetTheme(theme, color3)
        themes[theme] = color3
    end

    local CFAHubGui = Instance.new("ScreenGui")

    if intro == true then
        local Logo = Instance.new("ImageLabel")
        
