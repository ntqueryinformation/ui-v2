            Utility:TweenTransparency(Template,"bg",1)
            Utility:TweenTransparency(BarFrame,"bg",1)
            Utility:TweenTransparency(Bar,"bg",1)
            Utility:TweenTransparency(Header,"text",1)
            Utility:TweenTransparency(Message,"text",1)
